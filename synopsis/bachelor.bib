@article{barber2019,
  title = {Does {{Party Trump Ideology}}? {{Disentangling Party}} and {{Ideology}} in {{America}}},
  shorttitle = {Does {{Party Trump Ideology}}?},
  author = {Barber, Michael and Pope, Jeremy C.},
  year = {2019},
