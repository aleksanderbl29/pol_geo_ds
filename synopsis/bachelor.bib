@article{lofthouse2023,
  title = {The {{Continuing Case}} for a {{Polycentric Approach}} for {{Coping}} with {{Climate Change}}},
  author = {Lofthouse, Jordan K. and Herzberg, Roberta Q.},
  year = {2023},
  month = jan,
  journal = {Sustainability},
  volume = {15},
  number = {4},
  pages = {3770},
