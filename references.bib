@article{sands2020,
  title = {Local Exposure to Inequality Raises Support of People of Low Wealth for Taxing the Wealthy},
  author = {Sands, Melissa L. and family=Kadt, given=Daniel, prefix=de, useprefix=true},
  date = {2020-10},
