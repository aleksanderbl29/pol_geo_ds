@article{baccini2021,
  title = {Do Natural Disasters Help the Environment? {{How}} Voters Respond and What That Means},
  shorttitle = {Do Natural Disasters Help the Environment?},
  author = {Baccini, Leonardo and Leemann, Lucas},
  date = {2021-07},
  journaltitle = {Political Science Research and Methods},
  shortjournal = {Polit. Sci. Res. Methods},
  volume = {9},
  number = {3},
  pages = {468--484},
